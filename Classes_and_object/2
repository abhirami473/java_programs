class Box
{
  double width;
  double height;
  double depth;
void volume(){
System.out.println("volume is");
System.out.println(width*height*depth);
}
}
class Box3{
  public static void main(String args[]){
    Box mybox1=new Box();
    Box mybox2=new Box();
    mybox1.width=10;
    mybox1.height=20;
    mybox1.depth=15;
    mybox2.width=3;
    mybox2.height=2;
    mybox2.depth=5;
    mybox1.volume();
    mybox2.volume();
    
    
  }
  
}

OUTPUT

volume is
3000.0
volume is
30.0
