class Main
 {
  public static void main(String[] args) 
  {

    int num=29;
    String size;

    switch (num)
     {

      case 29:
        size = "Small";
        break;

      case 42:
        size = "Medium";
        break;

      
      case 44:
        size = "Large";
        break;

      case 48:
        size = "Extra Large";
        break;
      
      default:
        size = "Unknown";
        break;

    }
    System.out.println("Size: " + size);
  }
}

OUTPUT
Size: Small
