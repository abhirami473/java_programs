class Array
 {
   public static void main(String args[])
   { 
     int[] a = {12,4,5,2};
     
    System.out.println("first element:"+a[0]);
    System.out.println("second element:"+a[1]);
    System.out.println("third element:"+a[2]);
    System.out.println("fourth element:"+a[3]);
   }
 }

OUTPUT

first element:12
second element:4
third element:5
fourth element:2
